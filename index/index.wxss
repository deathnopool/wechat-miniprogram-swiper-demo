.swiper-container {
  position: relative;
  background-color: green;
  width: 200px;
  height: 200px;
}

.swiper-wrapper {
  width: 100%;
  height: 100%;
  display: flex;
  background-color: brown;
  position: relative;
  will-change: transform;
}

.swiper-container .item {
  width: 100%;
  height: 100%;
  flex-shrink: 0;
}